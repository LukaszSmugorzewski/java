    static int[] funkcjaSignum(int[] tab){
        int[] tablica = new int[tab.length];
        for (int i = 0; i < tab.length; i++){
            if (tab[i] > 0) {
                tablica[i] = 1;
            }
            else if (tab[i] < 0){
                tablica[i] = -1
            }
            else {
                tablica[i] = 0
            }
        }
    return tablica;
    }


    // Zadanie 21.
    // Wykorzystując funkcję z zadania 1. napisz funkcje najdluzszyCiagDodatnich(tab),
    // która wykonuje odpowiednie operacje na tablicy.

    static int zadanie_21(int[] tab) {
        int i = 0;
        int counter = 0;
        int result = 0;

        while (i < tab.length) {
            if (tab[i] > 0) {
                i++;
                counter++;
            } else {
                if (counter > result) {
                    result = counter;
                }
                counter = 0;
                i++;
            }
        }
        return result;
    }

